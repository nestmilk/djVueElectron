{
  "name": "djSvelteElectron",
  "version": "1.0.0",
  "main": "main.js",
  "description": "鼎晶审核系统",
  "author": {
    "name": "gong zhenhua",
    "email": "gongzhenhua1983@163.com"
  },
  "repository": "https://github.com/nestmilk/djSvelteElectron.git",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "electron-updater": "^4.2.0",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "wait-on": "^4.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "echarts": "^4.8.0",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.0",
    "elizabot": "0.0.2",
    "iconv-lite": "^0.5.1",
    "igv": "^2.3.5",
    "jquery": "^1.9.1",
    "js2excel": "^1.0.1",
    "popper.js": "^1.16.0",
    "sirv-cli": "^0.4.4",
    "socket.io-client": "^2.3.0",
    "socket.io-parser": "^3.4.0",
    "svelte-spa-router": "^2.0.0",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --host 0.0.0.0 --port 9999",
    "electron": "electron .",
    "devplus": "concurrently \"wait-on http://localhost:9999 && electron .\" \"cross-env BROWSER=none rollup -c -w\"",
    "buildMain": "webpack",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "cross-env GH_TOKEN=e0c58beb0096210ca18d300899636e20c0ccab98 electron-builder",
    "prepack": "npm run build && npm run buildMain",
    "predist": "npm run build && npm run buildMain",
    "prerelease": "npm run build && npm run buildMain"
  },
  "homepage": "./",
  "build": {
    "appId": "DingJing",
    "productName": "鼎晶审核系统",
    "copyright": "Copyright © 2020 ${author}",
    "extends": null,
    "files": [
      "public/**/*",
      "node_modules/**/*",
      "settings/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "./public/main.js"
    },
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "publisherName": "DingJing"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false
    },
    "mac": {
      "category": "public.app-category.productivity",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "dmg": {
      "background": "assets/appdmg.png",
      "icon": "assets/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 280,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 280,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    }
  },
  "private": true
}
